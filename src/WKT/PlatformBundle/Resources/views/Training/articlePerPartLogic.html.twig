{% if app.user and app.user.userArticlesRead is not empty %}
	{% for articleRead in app.user.userArticlesRead %}
	    {% if articleRead.article == article.entityArticle %}
	    	{% if articleRead.updated %}
	    		{% set edit = 'images/icones/edit.svg' %}
	    	{% else %}
	    		{% set finish = 'images/icones/finish.svg' %}
	    	{% endif %}
	    {% endif %}
	{% endfor %}
{% endif %}
